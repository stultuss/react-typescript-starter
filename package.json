{
  "name": "react-typescript-starter",
  "version": "1.0.0",
  "description": "A tool for quickly building the react app.",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "better-npm-run dev",
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "dev": "better-npm-run dev",
    "preview": "better-npm-run preview",
    "prod": "better-npm-run prod",
    "report": "better-npm-run report"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "rimraf dist && babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "preview": {
      "command": "npm run deploy && babel-node bin/server",
      "env": {
        "NODE_ENV": "preview",
        "DEBUG": "app:*"
      }
    },
    "prod": {
      "command": "npm run deploy && babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "report": {
      "command": "npm run deploy && babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "ANLYZER_REPORT": true,
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stultuss/react-typescript-starter.git"
  },
  "author": "fengjie",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/mz": "0.0.32",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-loadable": "^5.5.2",
    "@types/react-router": "^4.4.5",
    "@types/react-router-config": "^1.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/underscore": "^1.9.3",
    "axios": "^0.18.1",
    "mobx": "^5.14.2",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.1.1",
    "mz": "^2.7.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-loadable": "^5.5.0",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/debug": "0.0.31",
    "@types/jest": "^24.0.21",
    "@types/koa": "^2.0.51",
    "@types/koa-static": "^4.0.0",
    "@types/node": "^10.17.3",
    "@types/webpack-env": "^1.14.1",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.0",
    "better-npm-run": "^0.1.1",
    "caniuse-lite": "^1.0.30001008",
    "coveralls": "^3.0.7",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "debug": "^4.1.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.5.0",
