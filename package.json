{
  "name": "react-typescript-starter",
  "version": "1.0.0",
  "description": "A tool for quickly building the react app.",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "better-npm-run start",
    "start:dist": "better-npm-run start:dist",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "dist": "better-npm-run dist"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:dist": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && babel-node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "dist": {
      "command": "npm run deploy && babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niklaus0823/react_kit.git"
  },
  "author": "fengjie",
  "license": "MIT",
  "dependencies": {
    "@types/antd": "^1.0.0",
    "@types/mz": "0.0.32",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/underscore": "^1.8.9",
    "antd": "^3.11.0",
    "axios": "^0.18.0",
    "mobx": "^5.7.0",
    "mobx-react": "^5.4.2",
    "mz": "^2.7.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.2.0",
    "@types/debug": "0.0.31",
    "@types/koa": "^2.0.47",
    "@types/koa-static": "^4.0.0",
    "@types/node": "^10.12.12",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.0",
    "better-npm-run": "^0.1.1",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "debug": "^4.1.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.5.0",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "json-loader": "^0.5.7",
    "koa": "^2.6.2",
    "koa-compress": "^3.0.0",
    "koa-connect-history-api-fallback": "^0.3.1",
    "koa-proxy": "^0.9.0",
    "koa-static": "^5.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
